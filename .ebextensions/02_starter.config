container_commands:
  01_migrate:
    command: "python InspectTool/manage.py migrate"
    leader_only: true
  02_collectstatic:
    command: "python InspectTool/manage.py collectstatic --noinput"
  03_createsu:
    command: "python InspectTool/manage.py createsu"
    leader_only: true
option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "InspectTool.settings"
    "PYTHONPATH": "/opt/python/current/app/InspectTool:$PYTHONPATH"
    "ALLOWED_HOSTS": ".elasticbeanstalk.com"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: "InspectTool/wsgi.py"
